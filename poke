for a in "${firsts[@]}"; do
    for b in "${lasts[@]}"; do
        b_ll=${b:0:1}
        a_strip=${a%"$b_ll"}
        printf %s\\n "$a_strip$b"
    done
done | grep --color=none -xif - /usr/share/dict/words
